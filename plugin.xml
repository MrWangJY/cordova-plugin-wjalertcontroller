<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-wjalertcontroller" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>WJAlertController</name>
    <js-module name="WJAlertController" src="www/WJAlertController.js">
        <clobbers target="cordova.plugins.WJAlertController" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="WJAlertController">
                <param name="ios-package" value="WJAlertController" />
            </feature>
        </config-file>
        <source-file src="src/ios/WJAlertController.m" />
    </platform>
</plugin>
